{
  "name": "motion-detection-api",
  "version": "1.0.0",
  "description": "API for iCam-1000 motion detection.",
  "author": "Ben D. Botto and Ben B. Botto",
  "license": "MIT",
  "scripts": {
    "build": "rimraf dist && tsc -p tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "tsc-watch -p tsconfig.build.json --onSuccess \"node dist/main.js\"",
    "start:debug": "tsc-watch -p tsconfig.build.json --onSuccess \"node --inspect-brk dist/main.js\"",
    "start:prod": "node dist/main.js",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "migrate:up": "formn m -c ./connections.migrate.json up",
    "migrate:down": "formn m -c ./connections.migrate.json down",
    "migrate:create": "formn m -c ./connections.migrate.json create",
    "generate": "formn g -c ./connections.migrate.json src/entity",
    "test": "ts-node ./node_modules/.bin/jasmine --config=./jasmine.json --reporter=jasmine-console-reporter",
    "test:debug": "node --inspect-brk=0.0.0.0:9229 -r ts-node/register ./node_modules/.bin/jasmine --config=./jasmine.json --reporter=jasmine-console-reporter",
    "test:watch": "tsc-watch -p tsconfig.json --onSuccess \"npm run test\""
  },
  "dependencies": {
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/platform-express": "^6.0.0",
    "@sendgrid/mail": "^6.4.0",
    "@types/express": "^4.17.1",
    "@types/request-promise-native": "^1.0.16",
    "amqp-connection-manager": "^3.0.0",
    "amqplib": "^0.5.5",
    "bsy-error": "^2.0.0",
    "bsy-validation": "^2.5.0",
    "formn": "^5.9.1",
    "formn-cli": "^1.2.0",
    "formn-nestjs-utils": "^1.4.0",
    "moment": "^2.24.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "rimraf": "^2.6.2",
    "rxjs": "^6.3.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.0",
    "@types/jasmine": "^3.4.1",
    "@types/node": "11.13.4",
    "@types/supertest": "2.0.7",
    "jasmine": "^3.5.0",
    "jasmine-console-reporter": "^3.1.0",
    "prettier": "1.17.0",
    "supertest": "4.0.2",
    "ts-node": "8.1.0",
    "tsc-watch": "2.2.1",
    "tsconfig-paths": "3.8.0",
    "tslint": "5.16.0",
    "typescript": "3.4.3"
  }
}
